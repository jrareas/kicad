C_C5         LSET VN  {MST*133fF} 
C_C4         RSET VN  {MST*133fF} 
G_GLC6         RSETN VN VALUE { ((1+TANH(20*(V(LEW,
+  VN)-1.13))))*(200u)*(1+TANH(2.2/0.1V*(V(RSETN,VN)-0.2V)))/2 }
C_C6         RSETN VN  133fF 
GDC_I10         RSET VN VALUE {(1+TANH(2.2/0.1V*(V(RSET,VN)-2.5V)))/2*1uA}
E_E4         RSETG VN VALUE { (1+TANH(20*(V(RSET, VN)-1.56)))/2 }
